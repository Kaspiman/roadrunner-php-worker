{
    "name": "spiral/roadrunner-worker",
    "type": "library",
    "description": "RoadRunner: PHP worker",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        },
        {
            "name": "RoadRunner Community",
            "homepage": "https://github.com/spiral/roadrunner/graphs/contributors"
        }
    ],
    "bin": [
        "bin/rr"
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-sockets": "*",
        "symfony/console": "^5.1",
        "spiral/goridge": ">=3.0",
        "symfony/http-client": "^5.1",
        "symfony/polyfill-php80": "^1.22",
        "composer/semver": "^3.2",
        "composer-runtime-api": "^2.0"
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/phpstan": "~0.12",
        "symfony/var-dumper": "^5.1"
    },
    "scripts": {
        "analyze": "phpstan analyze -c ./phpstan.neon.dist --no-progress --ansi"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\RoadRunner\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
